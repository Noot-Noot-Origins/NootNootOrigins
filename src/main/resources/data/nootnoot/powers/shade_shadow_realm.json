{
  "type": "origins:multiple",
  "hidden": true,
  "decrease_visibility": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "scale set pehkui:visibility 0 @s",
          "permission_level": 4
        },
        {
          "type": "origins:execute_command",
          "command": "scale persist set pehkui:visibility true @s",
          "permission_level": 4
        },
        {
          "type": "origins:execute_command",
          "command": "scale delay set pehkui:visibility 0 @s",
          "permission_level": 4
        }
      ]
    },
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "reverse_visibility": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "scale reset pehkui:visibility",
      "permission_level": 4
    },
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle",
      "inverted": true
    }
  },
  "apply_shader": {
    "type": "origins:shader",
    "shader": "minecraft:shaders/post/desaturate.json",
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_block_use": {
    "type": "origins:prevent_block_use",
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_sleep": {
    "type": "origins:prevent_sleep",
    "message": "...",
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "invisibility": {
    "type": "origins:invisibility",
    "render_armor": false,
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_block_selection": {
    "type": "origins:prevent_block_selection",
    "block_condition": {
      "type": "origins:block",
      "block": "minecraft:air",
      "inverted": true
    },
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_damage_dealt": {
    "type": "origins:modify_damage_dealt",
    "modifier": {
      "operation": "multiply_base",
      "value": 2
    },
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_damage_taken": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "operation": "multiply_base",
      "value": -1
    },
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_burning": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:extinguish"
    },
    "interval": 5,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:on_fire"
        },
        {
          "type": "origins:power_active",
          "power": "nootnoot:shade_shadow_realm_toggle"
        }
      ]
    }
  },
  "prevent_collisions": {
    "type": "origins:prevent_entity_collision",
    "bientity_condition": {
      "type": "origins:target_condition",
      "condition": {
        "type": "origins:exists"
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_entity_use": {
    "type": "origins:prevent_entity_use",
    "bientity_condition": {
      "type": "origins:target_condition",
      "condition": {
        "type": "origins:exists"
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_game_event": {
    "type": "origins:prevent_game_event",
    "tag": "minecraft:vibrations",
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  },
  "prevent_item_use": {
    "type": "origins:prevent_item_use",
    "condition": {
      "type": "origins:power_active",
      "power": "nootnoot:shade_shadow_realm_toggle"
    }
  }
}