{
  "schemaVersion": 1,
  "id": "nootnoot",
  "version": "${version}",

  "name": "Noot Noot Origins",
  "description": "Custom Origins For The Noot Noot Origins Smp",
  "authors": [
    "Morgan"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/nootnoot/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "morgan.noot.noot.origins.NootNootOrigins"
    ],
    "client": [
      "morgan.noot.noot.origins.NootNootOriginsClient"
    ],
    "preLaunch": [
      "morgan.noot.noot.origins.NootNootOriginsPreLaunch"
    ],
    "cardinal-components": [
      "morgan.noot.noot.origins.NootNootOriginsComponents"
    ]
  },
  "mixins": [
    "nootnoot.mixins.json"
  ],

  "custom": {
    "cardinal-components": [
      "nootnoot:mount_position",
      "nootnoot:star_position",
      "nootnoot:star_velocity",
      "nootnoot:star_health",
      "nootnoot:star_world_key",
      "nootnoot:world_gravity",
      "nootnoot:entity_gravity",
      "nootnoot:entity_host_uuid"
    ]
  },

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
